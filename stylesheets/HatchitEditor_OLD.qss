
* {
	color: silver;
	font-family: "Ubuntu", "Arial";
}

QMainWindow {
	/*43443A*/
	background-color: #232B2B;


}

QDialog#Launcher *{
    font-size: 18px;
}

QListWidget#Launcher_RecentList {
    border: none;
    margin: 0px;
    padding: 0px;
}

QWidget#Launcher_Main {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                       stop:0 gray, stop:0.8 #636363);
}

QListView::item {
	padding: 5px;
}

QMenuBar *
{
	background-color: #43443A;
	color: silver;
	font-size: 14px;
}

QMenuBar::item
{
	background: transparent;
}

/*QMenuBar::item:hovered
{
  background: red;
}*/

QMenuBar::item:selected
{
	background: #9464E5;
	border: 1px solid #9464E5;
}

QMenuBar::item:pressed
{
	border: 1px solid #3A3939;
	background-color: #78879b;
	color: black;
	margin-bottom:-1px;
	padding-bottom:1px;
}

QMenu
{
  background-color: #43443A;
	border: 1px solid #3A3939;
	color: silver;
	margin: 2px;
}

QMenu::icon
{
	margin: 5px;
}

QMenu::item
{
  background-color: transparent;
	padding: 5px 30px 5px 30px;
	margin-left: 5px;
	border: 1px solid transparent; /* reserve space for selection border */
}

QMenu::item:selected
{
	color: black;
}

QToolBar {
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 gray, stop:0.8 #636363);
	border-top-right-radius: 7px;
	border-top-left-radius: 7px;
}

/************ QComboBox***********************/

QComboBox {
	background-color: #232B2B;
}

QComboBox:editable {
	background: #232B2B;
}

QComboBox:!editable{
     background: #9464E5;
}


/*********************************************/


QDialog {
	background-color: #232B2B;
}

QToolTip {
	background-color: #43443A;
	opacity: 150;
}

QTabWidget {
	background-color: #43443A;
}

QTabBar::tab {
	background-color: #636363;
	border-top-right-radius: 7px;
	border-top-left-radius: 7px;
	padding: 10px;
	min-width: 100px;
}

QTabBar::tab:hover {
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 gray, stop:0.8 #636363);
}

QTabBar::tab:selected {
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 gray, stop:0.8 #636363);
	color: #9464E5;
	font-weight: bold;
}


