 /*
 QTreeWidget {
	background-color: #232B2B;
 }
 */

 QTreeView {
	background-color: #232B2B;
 }

 QTreeView::item::selected {
    background-color: #9464E5;
}

QTreeView QHeaderView:section {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 gray, stop:0.8 #636363);
}

QTreeView::item:selected {
	background-color: #9464E5;
}

QScrollBar {
	background-color: #43443A;
	border-radius: 5px;
}

QDockWidget {
	titlebar-close-icon: url(:icons/dock_close.png);
	titlebar-normal-icon: url(:icons/dock_float.png);
}

QDockWidget::title
{
	text-align: center;
	spacing: 3px; /* spacing between items in the tool bar */
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 gray, stop:0.8 #636363);
}

QDockWidget::close-button, QDockWidget::float-button
{
	spacing: 1px; /* spacing between items in the tool bar */
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover
{
	border: none;
	background: transparent;
	titlebar-close-icon: url(:icons/dock_close_hover.png);
	titlebar-normal-icon: url(:icons/dock_float.png);
}


QPushButton {
	background-color: #43443A;
}

QPushButton::disabled {
	background-color: #232B2B;
}


QCheckBox::indicator {
     width: 14px;
     height: 14px;
 }

#CollapseCheck::indicator:checked
{
image: url(:icons/collapsed.png);
}
#CollapseCheck::indicator:unchecked
{
image: url(:icons/expanded.png);
}

#CollapseCheck::indicator:checked:hover
{
image: url(:icons/collapsed_hover.png);
}
#CollapseCheck::indicator:unchecked:hover
{
image: url(:icons/expanded_hover.png);
}
#CollapseCheck::indicator:checked:pressed
{
image: url(:icons/collapsed.png);
}
#CollapseCheck::indicator:unchecked:pressed
{
image: url(:icons/expanded.png);
}
#CollapseCheck::indicator:checked:disabled
{
image: url(:icons/collapsed.png);
}
#CollapseCheck::indicator:unchecked:disabled
{
image: url(:icons/expanded_disabled.png);
}

QWidget#CollapsePane {
	background-color: #32332B;
	border-radius: 8px;
	spacing: 1px;
}

QWidget#CollapsePaneHeader {
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 #4c4c4c, stop:0.12 #595959, stop:0.25 #666666, stop:0.25 #474747, stop:0.48 #000000,
				 stop:0.50 #000000, stop:0.52 #000000,
				 stop:0.55 #000000,  stop:0.62 #000000, stop:1.00 #000000);
	border-radius: 8px;
	padding-left: 6px;
	min-height: 28px;
	max-height: 28px;
}

QWidget#CollapsePaneHeader:hover {
	background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                 stop:0 #888888, stop:0.12 #757575, stop:0.25 #737373, stop:0.25 #5C5C5C, stop:0.48 #000000,
				 stop:0.50 #000000, stop:0.52 #000000,
				 stop:0.55 #000000,  stop:0.62 #000000, stop:1.00 #000000);
}

QLabel#CollapsePaneHeaderTitle {
	font-family: "Ubuntu", "Arial Black";
	font-size: 12px;
}
